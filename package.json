{
  "name": "hipchat-quotebot-ui",
  "version": "1.0.0",
  "description": "Cycles through quotes overlayed on top of images",
  "main": "handler.js",
  "scripts": {
    "start": "concurrently --kill-others \"nodemon --config nodemon.json\" \"ng serve\"",
    "start:serverless": "serverless offline start",
    "debug": "node --inspect ./node_modules/serverless/bin/serverless offline -s dev",
    "deploy": "del-cli dist --force && ng build --prod && serverless create_domain && serverless client deploy && serverless deploy -v",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
