{
  "name": "aws-nodejs-typescript",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "start": "concurrently --kill-others \"serverless offline start\" \"ng build -w\"",
    "debug": "node --debug ./node_modules/.bin/serverless offline -s dev",
    "deploy": "del-cli dist --force && ng build --prod && serverless client deploy && serverless deploy -v",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
