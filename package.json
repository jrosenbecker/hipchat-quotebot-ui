    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19",
    "aws-serverless-express": "^3.1.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
